{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:59000", // 更改為空閒端口
      "sslPort": 44370 // 設置 SSL 端口
    }
  },
  "profiles": {
    "http": {
      "commandName": "IISExpress", // 使用 IIS Express
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger", // 預設啟動 Swagger
      "applicationUrl": "http://localhost:5151", // 配置應用程式的端口
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "IISExpress", // 使用 IIS Express
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7090;http://localhost:5151", // 配置 HTTP 和 HTTPS
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress", // 使用 IIS Express
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
